r1 = int(input("Enter number of rows of first matrix: "))
c1 = int(input("Enter number of columns of first matrix: "))

r2 = int(input("Enter number of rows of second matrix: "))
c2 = int(input("Enter number of columns of second matrix: "))


if c1 != r2:
    print("Matrix multiplication not possible!")
else:
    print("Enter elements of first matrix:")
    A = []
    for i in range(r1):
        row = []
        for j in range(c1):
            row.append(int(input()))
        A.append(row)

    print("Enter elements of second matrix:")
    B = []
    for i in range(r2):
        row = []
        for j in range(c2):
            row.append(int(input()))
        
    C = [[0 for _ in range(c2)] for _ in range(r1)]

    
    for i in range(r1):
        for j in range(c2):
            for k in range(c1):
                C[i][j] += A[i][k] * B[k][j]

    
    print("Resultant Matrix:")
    for row in C:
        print(row)
